<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.domain.MenuInfo" table="menuinfo" >
        <id name="menuInfoId">
            <column name="menuInfoId" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="menuInfoName">
            <column name="menuInfoName" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
        <set name="roleRelationships" lazy="false" inverse="true">
            <key>
                <column name="menuInfoId" not-null="false"/>
            </key>
            <one-to-many not-found="ignore" class="com.domain.RoleRelationship"/>
        </set>
    </class>
</hibernate-mapping>