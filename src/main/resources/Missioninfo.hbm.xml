<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.domain.Missioninfo" table="missioninfo">
        <id name="missionId" type="java.lang.Integer">
            <column name="missionId" />
            <generator class="identity"/>
        </id>
        <property name="missionSubject" type="java.lang.String">
            <column name="missionSubject"  />
        </property>
        <property name="missionName" type="java.lang.String">
            <column name="missionName" />
        </property>
        <property name="missionLinkman" type="java.lang.String">
            <column name="missionLinkman"  />
        </property>
        <property name="completeTime" type="java.lang.String">
            <column name="completeTime" />
        </property>
        <property name="precedence" type="java.lang.String">
            <column name="precedence"/>
        </property>
        <property name="administer" type="java.lang.String">
            <column name="administer"  />
        </property>
        <property name="missionDescription" type="java.lang.String">
            <column name="missionDescription"  />
        </property>

    </class>
</hibernate-mapping>